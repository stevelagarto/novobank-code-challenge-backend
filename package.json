{
  "name": "novobank_code_challenge",
  "version": "0.1.0",
  "description": "Simple CRUD Project - Back End",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./node_modules/dotenv/config src/index.js",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "test": "NODE_ENV=test TZ=UTC jest --coverage",
    "test:watch": "NODE_ENV=test TZ=UTC jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevelagarto/novobank_code_challenge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevelagarto/novobank_code_challenge/issues"
  },
  "homepage": "https://github.com/stevelagarto/novobank_code_challenge#readme",
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "sequelize": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "testMatch": [
      "**/*.(spec).(js)"
    ]
  }
}
